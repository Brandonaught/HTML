      var starIcon = document.createElement("img");
        var starUrl = "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/staricon.png";
        starIcon.setAttribute("class", "favPng");
        starIcon.setAttribute("title", "Look I'm a star");
        starIcon.setAttribute("src", starUrl);
      //checkCookie();  

      $(function () {
  $('[data-toggle="tooltip"]').tooltip()
})
$(function () {
  $('[data-toggle="popover"]').popover()
})
  function setCookie(cname,cvalue,exdays) {
      var d = new Date();
      d.setTime(d.getTime() + (exdays*24*60*60*1000));
      var expires = "expires=" + d.toGMTString();
      document.cookie = cname+"="+cvalue+"; "+expires;

  }
  

  function getCookie(cname) {
      var name = cname + "=";
      var ca = document.cookie.split(';');
      for(var i=0; i<ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1);
          if (c.indexOf(name) == 0) {
              return c.substring(name.length, c.length);
          }
      }
      return "";
  }
  function checkCookie() {
      var btn;
      var detail;
      var fave = getCookie("URL");
      if (fave != "") {
          switch(fave)
          {
            case "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/1.jpg":
            btn = document.getElementById("btn1");
            $("#btn1").hide();
            btn1.parentNode.appendChild(starIcon);
            detail = document.getElementById("a1").href + "&fav=1";
            console.log("Page loaded favorite:" + detail);
            $("#a1").attr("href", detail);
            break;
            case "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/2.jpg":
            btn = document.getElementById("btn3");
            $("#btn3").hide();
            btn3.parentNode.appendChild(starIcon);
            detail = document.getElementById("a2").href + "&fav=1";
            console.log("Page loaded favorite:" + detail);
            $("#a2").attr("href", detail);
            break;
            case "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/3.jpg":
            btn = document.getElementById("btn5");
            $("#btn5").hide();
            btn5.parentNode.appendChild(starIcon);
            detail = document.getElementById("a3").href + "&fav=1";
            console.log("Page loaded favorite:" + detail);
            $("#a3").attr("href", detail);
            break;
            case "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/4.jpg":
            btn = document.getElementById("btn7");
            $("#btn7").hide();
            btn7.parentNode.appendChild(starIcon);
            detail = document.getElementById("a4").href + "&fav=1";
            console.log("Page loaded favorite:" + detail);
            $("#a4").attr("href", detail);
            break;
            case "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/5.jpg":
            btn = document.getElementById("btn9");
            $("#btn9").hide();
            btn9.parentNode.appendChild(starIcon);
            detail = document.getElementById("a5").href + "&fav=1";
            console.log("Page loaded favorite:" + detail);
            $("#a5").attr("href", detail);
            break;
            case "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/6.jpg":
            btn = document.getElementById("btn11");
            $("#btn11").hide();
            btn11.parentNode.appendChild(starIcon);
            detail = document.getElementById("a6").href + "&fav=1";
            console.log("Page loaded favorite:" + detail);
            $("#a6").attr("href", detail);
            break;
            default:
          }
          //alert("Your favorite site is: " + fave);
      } else {
         //user = prompt("Please enter your name:",""); this prompts for a user name if none is detected.
         if (fave != "" && fave != null) {
             setCookie("URL", fave, 30,checkCookie);
         }
      }
  }
  
  
        
       
      $(document).ready(function(){
                  $("#btn1").click(function(){
                    $("#btn1").hide();
                    btn1.parentNode.appendChild(starIcon);
                    cookieUrl = "https://s3.us-east-2.amazonaws.com/blam3110fall18/brandonphase3/1.jpg";
                    setCookie("URL", cookieUrl, 30);
                    console.log("cookieUrl =" + cookieUrl);
                    cookieChecker = 1;
                    detail = default1 + "&fav=1";
                    $("#a1").attr("href", detail);
                  })
                  $("#btn2").click(function(){
                    $("#btn1").show();
